generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  username  String   @unique
  password  String
  createAt  DateTime @default(now())
  updatedAt DateTime @updatedAt
  // messages  Message[]
  // profile   Profile?
}

